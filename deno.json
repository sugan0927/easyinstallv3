{
  "name": "easyinstall-deno",
  "version": "1.0.0",
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --watch src/index.ts",
    "deploy": "deployctl deploy --project=easyinstall-deno src/index.ts"
  },
  "deploy": {
    "project": "easyinstall-deno",
    "exclude": [".git", "README.md"],
    "include": ["src/index.ts"]
  }
}
